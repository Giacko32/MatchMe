<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="220" prefWidth="330" xmlns="http://javafx.com/javafx/11.0.14-internal"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.prova.matchme.GestionePartita.Interfacce.SelezionaSedeSportDataView">
    <ChoiceBox fx:id="scegliSede" layoutX="150.0" layoutY="28.0" prefWidth="150.0"/>
    <Label layoutX="32.0" layoutY="31.0" text="Seleziona sede:" textFill="#03b53f">
        <font>
            <Font name="System Bold" size="14.0"/>
        </font>
    </Label>
    <DatePicker fx:id="selezionaData" layoutX="150.0" layoutY="117.0" prefHeight="25.0" prefWidth="150.0"/>
    <Label layoutX="32.0" layoutY="120.0" text="Seleziona data:" textFill="#03b53f">
        <font>
            <Font name="System Bold" size="14.0"/>
        </font>
    </Label>
    <Button onMouseClicked="#ClickCerca" layoutX="249.0" layoutY="164.0" mnemonicParsing="false" text="Cerca">
        <font>
            <Font name="System Bold" size="12.800000190734863"/>
        </font>
        <cursor>
            <Cursor fx:constant="HAND"/>
        </cursor>
    </Button>
    <ChoiceBox fx:id="scegliSport" layoutX="150.0" layoutY="72.0" prefWidth="150.0"/>
    <Label layoutX="30.0" layoutY="75.0" text="Seleziona sport:" textFill="#03b53f">
        <font>
            <Font name="System Bold" size="14.0"/>
        </font>
    </Label>
</AnchorPane>
